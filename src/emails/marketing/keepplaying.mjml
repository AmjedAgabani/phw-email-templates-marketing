  <mj-body mj-class="dim-gray-bg" width="640px">

    <!-- Hero -->

    <mj-section mj-class="dark-gray-bg" background-position="left" background-repeat="no-repeat" background-size="600px" background-url="{{imgBaseUrl}}/markus-spiske-dbEkxltnNEc-unsplash.jpg" css-class="hero-image-background-position-desktop hero-desktop-padding" padding="0 20px">
      <mj-column>
        <mj-spacer css-class="hidden-desktop" height="34px" />
        <mj-spacer css-class="hidden-mobile" height="40px" />
        <mj-image align="center" href="https://www.google.co.uk" src="{{imgBaseUrl}}/phw-logo-white.png" width="113px" />
        <mj-spacer css-class="hidden-desktop" height="57px" />
        <mj-spacer css-class="hidden-mobile" height="75px" />
        <mj-text mj-class="white title-text uppercase">
          <span class="header-text-desktop">Keep<br> building</span>
        </mj-text>
        <mj-spacer css-class="hidden-desktop" height="30px" />
        <mj-spacer css-class="hidden-mobile" height="25px" />
