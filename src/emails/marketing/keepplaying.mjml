<mjml>
  <mj-head>
    <mj-attributes>
      <mj-all padding="0" />
      <mj-include path="../../styles/mj-class/bgcolors.mjml" />
      <mj-include path="../../styles/mj-class/colors.mjml" />
      <mj-include path="../../styles/mj-class/typography.mjml" />
    </mj-attributes>
    <mj-include path="../../styles/css/colors.mjml" />
    <mj-include path="../../styles/css/responsive.mjml" />
    <mj-include path="../../styles/css/typography.mjml" />
    <mj-preview>Preview</mj-preview>
    <mj-title>Title</mj-title>
    <mj-style>
      @media (min-width: 480px) {
      .hero-body-text-padding-desktop {
      padding-right: 260px !important;
      }
      .hero-desktop-padding > div > table > tbody > tr > td {
      padding: 0 40px !important;
      }
      .hero-image-background-position-desktop > div > table {
      background-size: cover !important;
      background-position: unset !important;
      }
      }

      @media (min-width: 480px) {
      .button-sizing-desktop > table > tbody > tr > td > a {
      width: 240px !important;
      }
      }

      @media (min-width: 480px) {
      .about-g-hub-desktop-padding > table> tbody > tr > td{
      padding-left: 43px !important;
      }
      }

      @media (min-width: 480px) {
      .collect-emails-desktop-image > div > table {
      background: url("{{imgBaseUrl}}/collect-desktop.png") !important;
      }
      }
